{
  "name": "node-server",
  "version": "1.0.0",
  "description": "Test exercise for node",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "build:test": "rm -rf ./dist && mkdir dist-tst",
    "build": "rm -rf ./dist && mkdir dist && cp ./package.json ./dist/package.json && npx tsc",
    "deploy:eb": "cd dist/ && zip -r node-server.zip . && mv ./node-server.zip .. && cd .. && eb deploy --message=node",
    "start": "node index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "test": "npx jest"
  },
  "author": "Lukasz Mroz",
  "license": "ISC",
  "dependencies": {
    "@babel/preset-env": "^7.23.8",
    "babel-jest": "^29.7.0",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-winston": "^4.2.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/body-parser": "^1.19.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.2.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
